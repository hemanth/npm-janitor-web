{
  "name": "npm-janitor-web",
  "private": "true",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production browserify main.js | uglifyjs -cm > public/bundle.js",
    "start": "ecstatic -p 8000 public",
    "watch": "watchify main.js -o public/bundle.js -dv"
  },
  "dependencies": {
    "babelify": "^6.1.3",
    "browserify": "^10.2.6",
    "ecstatic": "~0.8.0",
    "isomorphic-fetch": "^2.1.1",
    "react": "~0.13.3",
    "react-debounce-input": "^1.1.3",
    "react-masonry-component": "^1.0.4",
    "react-material-card": "0.0.2",
    "uglify-js": "^2.4.24",
    "watchify": "^3.2.3",
    "whatwg-fetch": "^0.9.0"
  }
}
